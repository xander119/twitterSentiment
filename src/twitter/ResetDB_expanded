IF OBJECT_ID('twitterResearch.tweet_set_two_processed1', 'U') IS NOT NULL
  DROP TABLE twitterResearch.tweet_set_two_processed1;

IF OBJECT_ID('twitterResearch.tweet_set_two_processed1_expanded', 'U') IS NOT NULL
  DROP TABLE twitterResearch.tweet_set_two_processed1_expanded;

